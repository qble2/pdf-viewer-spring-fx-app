<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<TitledPane fx:id="fileNotesViewTitledPane" animated="false" text="Notes" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qble2.pdf.viewer.gui.controller.NotesListViewController">
   <content>
      <VBox>
         <children>
            <Button focusTraversable="false" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addFileNote" text="New note">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true" styleClass="image-view-add-note" />
               </graphic>
            </Button>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <Accordion fx:id="fileNotesAccordion" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </content>
   <font>
      <Font size="14.0" />
   </font>
</TitledPane>
